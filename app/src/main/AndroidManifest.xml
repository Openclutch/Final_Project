<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="leeboelsma.final_project">

    <!-- Ask for permission so we can use the internet -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Weather"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CityWeather_Tej"></activity>
        <activity android:name=".MainEdActivity"></activity>
        <activity android:name=".DisplayActivity"></activity>
        <activity android:name=".Settings"></activity>
        <activity android:name=".alainMainActivity"></activity>

    </application>

</manifest>